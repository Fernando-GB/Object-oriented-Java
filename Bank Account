public class Account {
		
		double balance;
		int agency;
		int number;
		String holder;
		
		public void withdraw (double value) {
			this.balance += value;
		}
		
		public boolean withdraw (double value) {
			if (this.balance >= value) {
				this.balance -= value;
				return true;
			}else {
				return false;
			}
		}
}
