public class Account {
		
		double balance;
		int agency;
		int number;
		Customer holder;
		
		public void deposit (double value){
			this.balance += value;
		}
		
		public boolean withdraw (double value) {
			if (this.balance >= value) {
				this.balance -= value;
				return true;
			}else {
				return false;
			}
		}
		
		public boolean transfer(double value, Accoount destination) {
			if(this.balance >= value) {
				this.balance -= value;
				destination.deposit(value);
				return true;
			}else {
				return false;
			}
